{
  "name": "@dimasmith/together",
  "version": "0.1.3",
  "description": "Application for simultaneous photo viewing",
  "main": "server.js",
  "bin": {
    "together": "./cli.js"
  },
  "scripts": {
    "compile": "babel-node node_modules/webpack/bin/webpack.js",
    "prepublish": "rimraf ./dist && babel-node node_modules/webpack/bin/webpack.js",
    "pretest": "eslint test/**/*.js",
    "test": "mocha",
    "test:watch": "npm test -- --watch",
    "test:cov": "istanbul cover _mocha --report lcov",
    "test:codacy": "npm run test:cov && cat ./coverage/lcov.info | codacy-coverage",
    "start": "babel-node ./server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dimasmith/together.git"
  },
  "keywords": [
    "node",
    "collaboration",
    "websocket",
    "flux"
  ],
  "author": "Dmytro Kovalchuk <dimasmith@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dimasmith/together/issues"
  },
  "homepage": "https://github.com/dimasmith/together#readme",
